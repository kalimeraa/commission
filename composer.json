{
    "name": "kalimeraa/commission",
    "description": "commission task",
    "keywords": [],
    "type": "project",
    "autoload": {
        "psr-4": {
            "Kalimeraa\\Commission\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kalimeraa\\Commission\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.26",
        "friendsofphp/php-cs-fixer": "3.13"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "scripts": {
        "phpunit": "phpunit",
        "fix-cs": "php-cs-fixer fix",
        "test-cs": "php-cs-fixer fix --dry-run -v",
        "test": ["@phpunit", "@test-cs"]
    }
}
